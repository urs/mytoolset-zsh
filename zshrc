# ~/.zshrc
#
# @package mytoolset
# @subpackage zsh
# @author thiagoalessio

export ZSH_HOME="$HOME/Projects/Open Source/mytoolset-zsh"

source $ZSH_HOME/aliases.zsh
source $ZSH_HOME/colors.zsh
source $ZSH_HOME/completion.zsh
source $ZSH_HOME/history.zsh
source $ZSH_HOME/vcs.zsh
source $ZSH_HOME/prompt.zsh
source $ZSH_HOME/environment.zsh
